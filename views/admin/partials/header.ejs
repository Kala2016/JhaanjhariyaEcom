<script>
    // Add event listener to the logout button
    document.getElementById('logoutButton').addEventListener('click', function (event) {
        // Prevent the default behavior of the link
        event.preventDefault();

        // Check if SweetAlert2 is defined
        if (typeof Swal !== 'undefined') {
            // Display the flash message
            Swal.fire({
                title: 'Successfully logged out!',
                icon: 'success',
                showConfirmButton: false,
                timer: 2000
            });
        } else {
            // Handle case when SweetAlert2 is not defined
            console.error('SweetAlert2 is not defined');
        }

        // After showing the flash message, proceed with logout redirection
        setTimeout(function () {
            window.location.href = "admin"; // Redirect to the logout page
        }, 2000); // Adjust the delay as needed
    });
</script>

